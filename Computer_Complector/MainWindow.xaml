							<Setter Property="ItemTemplate" Value="{StaticResource BODYElements}"/>
							<Setter Property="ItemsSource" Value="{Binding Bodies}"/>
						</DataTrigger>
					</Style.Triggers>
				</Style>
			</ListBox.Style>
			<ListBox.ItemContainerStyle>
				<Style TargetType="ListBoxItem">
					<Style.Setters>
						<Setter Property="BorderBrush">
							<Setter.Value>
								<SolidColorBrush/>
							</Setter.Value>
						</Setter>
						<Setter Property="BorderThickness" Value="0"/>
						<Setter Property="Background">
							<Setter.Value>
								<SolidColorBrush/>
							</Setter.Value>
						</Setter>
						<Setter Property="Effect" Value="{x:Null}"/>
						<Setter Property="Margin" Value="0,0,0,5"/>
						<Setter Property="Focusable" Value="False"/>
						<Setter Property="Template" Value="{StaticResource flatListBoxItem}"/>
					</Style.Setters>
					<Style.Triggers>
						<DataTrigger Binding="{Binding IsSelected}" Value="True">
							<DataTrigger.Setters>
								<Setter Property="IsSelected" Value="True"/>
							</DataTrigger.Setters>
						</DataTrigger>
						<Trigger Property="IsSelected" Value="True">
							<Trigger.Setters>
								<Setter Property="Effect" Value="{StaticResource SelectedItem}"/>
								<Setter Property="Background" Value="#11FFFFFF"/>
							</Trigger.Setters>
						</Trigger>
					</Style.Triggers>
				</Style>
			</ListBox.ItemContainerStyle>
		</ListBox>
		</Grid>

		<!--#endregion-->

		<Grid Grid.Row="3" Grid.RowSpan="2" Grid.Column="2">
			<Grid.RowDefinitions>
				<RowDefinition Height="2*"/>
				<RowDefinition Height="16*"/>
			</Grid.RowDefinitions>
			<Grid x:Name="SelectionActionPanel" Margin="20,10,30,10">
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="1*"/>
					<ColumnDefinition Width="1*"/>
				</Grid.ColumnDefinitions>
				<Border Margin="20,5,10,5" CornerRadius="10" BorderThickness="1,0,1,0" BorderBrush="White">
					<Border.Style>
						<Style TargetType="Border">
							<Style.Setters>
								<Setter Property="Effect" Value="{x:Null}"/>
								<Setter Property="Cursor" Value="Hand"/>
							</Style.Setters>
							<Style.Triggers>
								<Trigger Property="IsMouseOver" Value="True">
									<Trigger.Setters>
										<Setter Property="Effect" Value="{StaticResource HoverImg}"/>
									</Trigger.Setters>
								</Trigger>
							</Style.Triggers>
						</Style>
					</Border.Style>
					<Button Margin="5" Template="{StaticResource flatBtn}" Command="{Binding Path=OpenFile}">
						<Button.Style>
							<Style TargetType="Button">
								<Style.Setters>
									<Setter Property="Background" Value="{x:Null}"/>
									<Setter Property="BorderBrush" Value="{x:Null}"/>
								</Style.Setters>
							</Style>
						</Button.Style>
						<Image HorizontalAlignment="Center" VerticalAlignment="Center" Source="Resources/Images/Open.png"/>
					</Button>
				</Border>
				<Border Margin="10,5,20,5" CornerRadius="10" BorderThickness="1,0,1,0" BorderBrush="White" Grid.Column="1">
					<Border.Style>
						<Style TargetType="Border">
							<Style.Setters>
								<Setter Property="Effect" Value="{x:Null}"/>
								<Setter Property="Cursor" Value="Hand"/>
							</Style.Setters>
							<Style.Triggers>
								<Trigger Property="IsMouseOver" Value="True">
									<Trigger.Setters>
										<Setter Property="Effect" Value="{StaticResource HoverImg}"/>
									</Trigger.Setters>
								</Trigger>
							</Style.Triggers>
						</Style>
					</Border.Style>
					<Button Margin="5" Template="{StaticResource flatBtn}" Command="{Binding SaveFile}">
						<Button.Style>
							<Style TargetType="Button">
								<Style.Setters>
									<Setter Property="Background" Value="{x:Null}"/>
									<Setter Property="BorderBrush" Value="{x:Null}"/>
								</Style.Setters>
							</Style>
						</Button.Style>
						<Image Source="Resources/Images/Save.png" HorizontalAlignment="Center" VerticalAlignment="Center"/>
					</Button>
				</Border>
			</Grid>
			<ScrollViewer Grid.Row="1" Margin="10">
				<ListView x:Name="selectedItemsListView" Style="{StaticResource selectedListViewContainer}">
					<ListView Style="{StaticResource selectedViewList}" ItemsSource="{Binding Path=Item1}" ItemTemplate="{StaticResource CPUElementSelected}"/>
					<ListView Style="{StaticResource selectedViewList}" ItemsSource="{Binding Path=Item2}" ItemTemplate="{StaticResource MOTHERBOARDElementSelected}"/>
					<ListView Style="{StaticResource selectedViewList}" ItemsSource="{Binding Path=Item3}" ItemTemplate="{StaticResource VIDEOCARDElementSelected}"/>
					<ListView Style="{StaticResource selectedViewList}" ItemsSource="{Binding Path=Item4}" ItemTemplate="{StaticResource RAMElementSelected}"/>
					<ListView Style="{StaticResource selectedViewList}" ItemsSource="{Binding Path=Item5}" ItemTemplate="{StaticResource CHARGERElementSelected}"/>
					<ListView Style="{StaticResource selectedViewList}" ItemsSource="{Binding Path=Item6}" ItemTemplate="{StaticResource COOLERElementSelected}"/>
					<ListView Style="{StaticResource selectedViewList}" ItemsSource="{Binding Path=Item7}" ItemTemplate="{StaticResource SSDElementSelected}"/>
					<ListView Style="{StaticResource selectedViewList}" ItemsSource="{Binding Path=Item8}" ItemTemplate="{StaticResource HDDElementSelected}"/>
					<ListView Style="{StaticResource selectedViewList}" ItemsSource="{Binding Path=Item9}" ItemTemplate="{StaticResource BODYElementSelected}"/>
				</ListView>
			</ScrollViewer>
		</Grid>
	</Grid>
</Window>
