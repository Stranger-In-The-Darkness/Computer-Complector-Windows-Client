		<!--#region Fields panel-->

		<Border 
			Style="{StaticResource fieldsListLoader}" 
			Grid.Row="4" 
			Grid.RowSpan="2" 
			RenderTransformOrigin="0.5,0.5"/>
		<ListView 
            x:Name="FiledsListBox" 
            Focusable="False" 
            Margin="10" 
            Grid.Row="3" 
            Grid.RowSpan="3" 
            ScrollViewer.VerticalScrollBarVisibility="Visible" 
            ScrollViewer.HorizontalScrollBarVisibility="Hidden" 
            Background="{x:Null}" 
            BorderBrush="{x:Null}" 
            Foreground="{x:Null}"
            ScrollViewer.CanContentScroll="False">
			<ListView.Style>
				<Style TargetType="ListView">
					<Style.Setters>
						<Setter Property="ItemsSource" Value="{x:Null}"/>
					</Style.Setters>
					<Style.Triggers>
						<DataTrigger Binding="{Binding Path=SelectedTab}" Value="0">
							<Setter Property="ItemsSource" Value="{Binding CPUFields}"/>
						</DataTrigger>
						<DataTrigger Binding="{Binding Path=SelectedTab}" Value="1">
							<Setter Property="ItemsSource" Value="{Binding MOTHERBOARDFields}"/>
						</DataTrigger>
						<DataTrigger Binding="{Binding Path=SelectedTab}" Value="2">
							<Setter Property="ItemsSource" Value="{Binding VIDEOCARDFields}"/>
						</DataTrigger>
						<DataTrigger Binding="{Binding Path=SelectedTab}" Value="3">
							<Setter Property="ItemsSource" Value="{Binding RAMFields}"/>
						</DataTrigger>
						<DataTrigger Binding="{Binding Path=SelectedTab}" Value="4">
							<Setter Property="ItemsSource" Value="{Binding CHARGERFields}"/>
						</DataTrigger>
						<DataTrigger Binding="{Binding Path=SelectedTab}" Value="5">
							<Setter Property="ItemsSource" Value="{Binding COOLERFields}"/>
						</DataTrigger>
						<DataTrigger Binding="{Binding Path=SelectedTab}" Value="6">
							<Setter Property="ItemsSource" Value="{Binding SSDFields}"/>
						</DataTrigger>
						<DataTrigger Binding="{Binding Path=SelectedTab}" Value="7">
							<Setter Property="ItemsSource" Value="{Binding HDDFields}"/>
						</DataTrigger>
						<DataTrigger Binding="{Binding Path=SelectedTab}" Value="8">
							<Setter Property="ItemsSource" Value="{Binding BODYFields}"/>
						</DataTrigger>
					</Style.Triggers>
				</Style>
			</ListView.Style>
			<ListView.ItemContainerStyle>
				<Style TargetType="ListViewItem">
					<Style.Setters>
						<Setter Property="Focusable" Value="False"/>
						<Setter Property="Template" Value="{StaticResource flatListViewItem}"/>
					</Style.Setters>
				</Style>
			</ListView.ItemContainerStyle>
			<ListView.ItemTemplate>
				<DataTemplate>
					<Expander Background="{x:Null}" BorderBrush="{x:Null}" Margin="10, 0, 0, 0">
						<Expander.Header>
							<StackPanel Grid.ColumnSpan="2" Orientation="Horizontal" Margin="0,5">
								<Border BorderBrush="White" CornerRadius="100" BorderThickness="1" Width="20" Height="20" DataContext="{Binding Path=Value}">
									<Border.Style>
										<Style TargetType="Border">
											<Style.Setters>
												<Setter Property="Effect">
													<Setter.Value>
														<DropShadowEffect ShadowDepth="0" Color="#00000000" BlurRadius="0"/>
													</Setter.Value>
												</Setter>
												<Setter Property="Cursor" Value="Hand"/>
												<Setter Property="Visibility" Value="Visible"/>
											</Style.Setters>
											<Style.Triggers>
												<Trigger Property="IsMouseOver" Value="True">
													<Trigger.Setters>
														<Setter Property="Effect">
															<Setter.Value>
																<DropShadowEffect ShadowDepth="0" Color="#FF46FF00" BlurRadius="10"/>
															</Setter.Value>
														</Setter>
													</Trigger.Setters>
												</Trigger>
												<DataTrigger Binding="{Binding Path=Addition}" Value="False">
													<DataTrigger.Setters>
														<Setter Property="Visibility" Value="Collapsed"/>
													</DataTrigger.Setters>
												</DataTrigger>
											</Style.Triggers>
										</Style>
									</Border.Style>
									<Label Foreground="White" Background="{x:Null}" Padding="0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">
										?
										<Label.Style>
											<Style TargetType="Label">
												<Style.Setters>
													<Setter Property="ToolTip" Value="..."/>
												</Style.Setters>
												<Style.Triggers>
													<DataTrigger Binding="{Binding Path = Item1}" Value="True">
														<DataTrigger.Setters>
															<Setter Property="ToolTip" Value="{Binding Path = Item2}"/>
														</DataTrigger.Setters>
													</DataTrigger>
												</Style.Triggers>
											</Style>
										</Label.Style>
									</Label>
								</Border>
								<Label Foreground="White" Margin="10,0" Content="{Binding Path=Value.Text}"/>
							</StackPanel>
						</Expander.Header>
						<ListView Tag="{Binding Path=Key}" ItemsSource="{Binding Path=Value.Values}" Background="{x:Null}" BorderBrush="{x:Null}">
							<ListView.ItemContainerStyle>
								<Style TargetType="ListViewItem">
									<Style.Setters>
										<Setter Property="Focusable" Value="False"/>
										<Setter Property="Template" Value="{StaticResource flatListViewItem}"/>
									</Style.Setters>
								</Style>
							</ListView.ItemContainerStyle>
							<ListView.ItemTemplate>
								<DataTemplate>
									<CheckBox 
                                        Margin="20,0,0,0"
                                        Content="{Binding}" 
                                        Tag="{Binding Path=DataContext.Key, RelativeSource={RelativeSource AncestorType=ListView}}" 
                                        Checked="ToggleFilter"
                                        Unchecked="ToggleFilter"
                                        Foreground="White"/>
								</DataTemplate>
							</ListView.ItemTemplate>
						</ListView>
					</Expander>
				</DataTemplate>
			</ListView.ItemTemplate>
		</ListView>

